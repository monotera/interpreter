main miprograma {
	//Programa de prueba de la clase Teoria de la computacion
	
	var x;
	x = 5;
	
	var a;
	a = "NO";
	
	var cent;
	cent = true;
	
	cout ("Primer Condicional");
	
	if(cent){
		cout ( 2+2*4 );
	} else {
		cout ( x );
